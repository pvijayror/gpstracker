.container
  .row-fluid
    - zoom = @json.length > 100 ? true : false
    = gmaps("markers" => {"data" => @json}, "map_options" => {"processing" => "json", "auto_adjust" => true, "auto_zoom" => zoom, "zoom" => 6}) if !@json.nil? 
    = link_to "Back", user_devices_path, :class => "btn btn-warning"
